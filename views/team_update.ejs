<%-include('header') %>
        

              <h4 class="fw-bold py-3 mb-4"><span class="text-muted fw-light">Forms /</span> Basic Inputs</h4>
                <!-- Form controls -->
              <form method="post" action="/team/EditRecord" enctype="multipart/form-data">
                <input type="hidden" name="editId" value="<%= data.id %>">
                <div class="col-md-6">
                  <div class="card mb-4">
                    <h5 class="card-header">Form Controls</h5>
                    <div class="card-body">
                        <div class="mb-3">
                            <label for="defaultFormControlInput" class="form-label">name</label>
                            <input type="text" class="form-control" name="name" value="<%= data.name %>" id="defaultFormControlInput" placeholder="John Doe"aria-describedby="defaultFormControlHelp"/>
                        </div>
                        <div class="mb-3">
                            <label for="defaultFormControlInput" class="form-label">Post</label>
                            <input type="text" class="form-control" name="post" value="<%= data.post %>" id="defaultFormControlInput" placeholder="John Doe"aria-describedby="defaultFormControlHelp"/>
                        </div>
                        <div>
                            <label for="exampleFormControlTextarea1" class="form-label">About</label>
                            <textarea class="form-control" id="exampleFormControlTextarea1" name="About" rows="3"><%=data.About%></textarea>
                        </div>
                      <div class="mb-3">
                        <label for="formFile" class="form-label">Image</label>
                        <input class="form-control" type="file" name="image" id="formFile" />
                        <img src="<%= data.image  %>" width="80" height="80" alt="" srcset="">
                      </div>
                    
                      <div class="mt-3">
                        <input type="submit" value="Add Admin" name="submit" class="btn btn-danger">
                      </div>
                      
                    </div>
                  </div>
                </div>
              </form>

           <%-include('footer') %>